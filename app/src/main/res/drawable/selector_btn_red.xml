<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">

            <corners android:radius="@dimen/selector_btn_red_btn_radius"></corners>
            <solid android:color="@color/btn_selector_btn_red_selected"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">

            <corners android:radius="@dimen/circular_btn_radius"></corners>
            <solid android:color="@color/btn_selector_btn_red"/>
        </shape>
    </item>
</selector>
